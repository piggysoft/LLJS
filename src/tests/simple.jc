struct Node {
  Node *next, int value
}

var Node *head = new Node, *tail = head;

function int max (int x, int y) {
  return x > y ? y : x;
}


function int add (int value) {
  var Node * next = new Node;
  next->value = value = 2;
  tail->next = next;
  tail = next;
  return value;
}

